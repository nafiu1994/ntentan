<?xml version="1.0" encoding="utf-8"?>
<phpunit bootstrap="bootstrap.php">
    <php>
        <var name="config" value="local" />
    </php>    
    <testsuites>
        <testsuite name="Caching testsuite">
            <directory>cases/lib/caching</directory>
        </testsuite>
        <testsuite name="Models testsuite">
            <directory>cases/lib/models</directory>
        </testsuite>
    </testsuites>  
    <logging>
      <log type="coverage-html" target="/tmp/report" charset="UTF-8"
           highlight="false" lowUpperBound="35" highLowerBound="70"/>
    </logging>     
</phpunit>